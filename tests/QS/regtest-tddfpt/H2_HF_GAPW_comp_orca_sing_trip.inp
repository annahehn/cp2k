&GLOBAL
 RUN_TYPE ENERGY
 PROJECT_NAME ne
 PRINT_LEVEL MEDIUM
&END GLOBAL
&FORCE_EVAL
&PROPERTIES
 &TDDFPT
  NSTATES 5
  MAX_ITER 20
  MAX_KV 60
  CONVERGENCE 1.0E-6
  KERNEL FULL
  &XC
   &XC_FUNCTIONAL NONE
   &END XC_FUNCTIONAL
       &HF
       FRACTION 1.0
      &END HF
  &END XC
  &SOC
   EPS_FILTER 1.0E-15
  &END SOC
  &PRINT
  &SOC_PRINT
   SPLITTING F
   SOME F
   UNIT_wn
  &END SOC_PRINT
  &END PRINT
 &END TDDFPT
&END PROPERTIES
&DFT
BASIS_SET_FILE_NAME EMSL_BASIS_SETS
POTENTIAL_FILE_NAME POTENTIAL
&SCF
 SCF_GUESS ATOMIC
 EPS_SCF 1.0E-7
 MAX_SCF 20
&OUTER_SCF
 EPS_SCF 1.0E-7
 MAX_SCF 200
&END OUTER_SCF
&OT
 PRECONDITIONER FULL_ALL
 MINIMIZER DIIS
&END OT
&END SCF
&MGRID
 CUTOFF 400
 REL_CUTOFF 40
&END
&QS
 METHOD GAPW
&END QS
&POISSON
 PERIODIC NONE
 PSOLVER MT
&END POISSON
 &XC
  &XC_FUNCTIONAL NONE
  &END XC_FUNCTIONAL
      &HF
       FRACTION 1.0
      &END HF
 &END XC
&END DFT
&SUBSYS
 &CELL
  ABC 5.0 5.0 5.0
  PERIODIC NONE
 &END CELL
 &TOPOLOGY
  &CENTER_COORDINATES
  &END 
 &END TOPOLOGY
 &COORD
  H 0.0 0.0 0.0
  H 0.0 0.0 1.0
 &END COORD
 &KIND Ne
  BASIS_SET Ahlrichs-def2-SVP
  POTENTIAL ALL
 &END KIND
 &KIND H
  BASIS_SET Ahlrichs-def2-SVP
  POTENTIAL ALL
 &END KIND
&END SUBSYS
&END FORCE_EVAL
